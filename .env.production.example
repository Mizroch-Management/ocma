# Production Environment Variables
# Copy this file to .env.production and fill in your values

# Supabase Configuration
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key-here

# CDN Configuration
VITE_CDN_URL=https://cdn.yourdomain.com

# AI Platform Keys (keep these secure!)
VITE_OPENAI_API_KEY=sk-...
VITE_ANTHROPIC_API_KEY=sk-ant-...
VITE_GOOGLE_AI_API_KEY=...
VITE_STABILITY_API_KEY=...

# Social Platform OAuth (for production)
VITE_INSTAGRAM_CLIENT_ID=...
VITE_INSTAGRAM_CLIENT_SECRET=...
VITE_TWITTER_CLIENT_ID=...
VITE_TWITTER_CLIENT_SECRET=...
VITE_LINKEDIN_CLIENT_ID=...
VITE_LINKEDIN_CLIENT_SECRET=...

# Analytics
VITE_GA_TRACKING_ID=G-...
VITE_MIXPANEL_TOKEN=...

# Feature Flags
VITE_ENABLE_AI_FEATURES=true
VITE_ENABLE_ENTERPRISE=true
VITE_ENABLE_ANALYTICS=true

# App Configuration
VITE_APP_NAME=OCMA
VITE_APP_URL=https://ocma.app
VITE_API_URL=https://api.ocma.app