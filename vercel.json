{
  "headers": [
    {
      "source": "/:asset(.*\\.(js|css|png|jpg|jpeg|gif|ico|svg|webp|woff|woff2))",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    }
  ],
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/$1" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
